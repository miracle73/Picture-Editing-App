{
  "name": "pichub",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "generate-apk": "eas build -p android --profile preview"
  },
  "jest": {
    "preset": "jest-expo",
    "verbose": true,
    "fakeTimers": {
      "enableGlobally": true
    },
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.jsx",
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@?ptomasroos/react-native-multi-slider|@?react-navigation)",
      "node_modules/(?!((jest-)?react-native|react-test-renderer|react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|native-base|react-native-svg|@rneui/.*)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@hookform/resolvers": "^3.3.4",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-navigation/elements": "^1.3.21",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.20",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "expo": "^49.0.15",
    "expo-checkbox": "~2.4.0",
    "expo-font": "~11.4.0",
    "expo-linear-gradient": "~12.3.0",
    "expo-random": "~13.2.0",
    "expo-secure-store": "~12.3.1",
    "expo-status-bar": "~1.6.0",
    "jest": "^29.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.3",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "^13.9.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-web": "~0.19.6",
    "yup": "^1.3.3",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^18.0.7",
    "jest-expo": "~49.0.0",
    "react-native-dotenv": "^3.4.8",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
